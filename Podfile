# Uncomment the next line to define a global platform for your project
 platform :ios, '10.0'

target 'cycling app' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  use_frameworks!

  # Pods for cycling app
  pod 'Realm'
  target 'cycling appTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Realm/Headers'
  end

  target 'cycling appUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
